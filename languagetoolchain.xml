<?xml version="1.0" encoding="ISO-8859-1"?>
<mp:languageToolchain xsi:schemaLocation="http://crownking/mplab mplab/languageToolchain.xsd" mp:id="SectionInsert0.1" mp:name="SectionInsert" mp:hostplatforms="windows,linux,unix,mac,none" xmlns:opt="http://crownking/opt" xmlns:mp="http://crownking/mplab" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<mp:version mp:name="0.1"/>

<mp:customizerNode mp:idref="SecInsPrePreProc" mp:name="" >
  <mp:customizerNode mp:idref="SecInsPrePreProc" mp:name="" />
  <mp:customizerNode mp:idref="GenericLinker" mp:name="" />
</mp:customizerNode>



<mp:assembler mp:file="mpasme.py" />
<mp:linker mp:file="" />

<mp:makefileWriter>

  <mp:inputToOutputExtensionMap mp:src="asm" mp:dst="o" />
    <mp:outputFilenameSuffixProvider mp:value="hex" />
    <mp:outputFilenameSuffixProvider mp:value="hex" />

  <mp:makeRule mp:id="compile" mp:prerequisites=".[aA][sS][mM]$">
    <mp:textEmitter mp:text="${MKDIR} ${CURRENT_QUOTED_IF_SPACED_OBJECT_DIR_UNIX_SLASHES} ${NEWLINE}"/>
    <mp:textEmitter mp:text="${RM} ${CURRENT_QUOTED_IF_SPACED_OBJECT_FILE} ${NEWLINE}"/>
    <mp:textEmitter mp:text="${MP_CC} "/>
    <mp:textEmitter mp:onlydebug="true" mp:text=" --debug"/>
    <!-- <mp:textEmitter mp:text=" -c"/> -->
    <!-- <mp:textEmitter mp:text=" -m${PROCESSOR_FAMILY}"/> -->
    <mp:textEmitter mp:text=" -p${PROCESSOR_NAME}"/>
    <mp:textEmitter mp:text=" ${CURRENT_QUOTED_IF_SPACED_SOURCE_FILE} "/>
    <mp:textEmitter mp:text=" -o${CURRENT_QUOTED_IF_SPACED_OBJECT_FILE}"/>
    <mp:textEmitter mp:text="${NEWLINE}"/>
  </mp:makeRule>

  <mp:makeRule mp:id="link" mp:target="${FINAL_IMAGE_NAME}" mp:prerequisites="${CURRENT_OBJECT_FILES}">
    <mp:textEmitter mp:text="${MKDIR} ${FINAL_TARGET_DIR} ${NEWLINE}"/>
    <mp:textEmitter mp:text="${MP_CC} "/>
    <!-- <mp:textEmitter mp:text="-Wl-c -Wl-m "/> -->
    <mp:textEmitter mp:text=" -m${PROCESSOR_FAMILY}"/>
    <mp:textEmitter mp:text=" -p${PROCESSOR_NAME}"/>
    <mp:textEmitter mp:text=" ${CURRENT_QUOTED_IF_SPACED_OBJECT_FILES}" mp:separator=" "/>
    <mp:textEmitter mp:text=" -o${FINAL_IMAGE_NAME} "/>
  </mp:makeRule>

</mp:makefileWriter>

</mp:languageToolchain>
